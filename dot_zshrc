# zstyle ':znap:*' repos-dir ~/.config/znap
# source ~/zsh-snap/znap.zsh
echo "running zshrc"

source $DOTZSH/config.zsh
# source $DOTZSH/omzconfig.zsh

# source $ZSH/oh-my-zsh.sh

source $DOTZSH/aliases.zsh
source $DOTZSH/functions.zsh
source $DOTZSH/bindings.zsh

eval "$(starship init zsh)"

if [[ -f $DOTZSH/lrc.zsh ]]; then
    source $DOTZSH/lrc.zsh 
fi 

export PATH=$PATH:/Users/mstewart/.spicetify

autoload edit-command-line
zle -N edit-command-line
bindkey -M vicmd v edit-command-line

[ -f ~/.fzf.zsh ] && source ~/.fzf.zsh

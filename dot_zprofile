_dotfileLog "sourcing .zprofile" 2>/dev/null

if [[ ! -d "${TMPDIR}" ]]; then
  mkdir -m 700 "${TMPDIR}"
fi


### EXPORT SOURCE
export DOTZSH="$HOME/.config/zsh"
source $DOTZSH/exports.zsh

### PATH SOURCE
source $DOTZSH/path.zsh

### .ENV SOURCE
if [[ -f $DOTZSH/.env ]]; then
    source $DOTZSH/.env
fi 
if [[ -f $HOME/.env ]]; then
    source $HOME/.env
fi 

typeset -gU cdpath fpath mailpath path


